{
  "kind": "collectionType",
  "collectionName": "months_data",
  "info": {
    "singularName": "month-data",
    "pluralName": "months-data",
    "displayName": "MonthData",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "monthIdentifier": {
      "type": "uid",
      "required": true
    },
    "month": {
      "type": "enumeration",
      "enum": [
        "january",
        "february",
        "march",
        "april",
        "may",
        "june",
        "july",
        "august",
        "september",
        "october",
        "november",
        "december"
      ],
      "required": true
    },
    "year": {
      "type": "integer",
      "max": 3000,
      "required": true
    },
    "totalDays": {
      "type": "integer",
      "max": 31,
      "required": true
    },
    "workingDays": {
      "type": "integer",
      "required": true
    },
    "monthly_salaries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::monthly-salary.monthly-salary",
      "mappedBy": "month_data"
    },
    "holidayCount": {
      "type": "integer"
    }
  }
}
